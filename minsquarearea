#include<bits/stdc++.h>
#include<cmath>
using namespace std;
int main()
{
	long long int t;
	cin>>t;
	while(t--)
	{
		long long int n;
		cin>>n;
		long long int a[n],b[n],i;
		for(i=0;i<n;i++)
		{
			cin>>a[i]>>b[i];
		}
		sort(a,a+n);
		sort(b,b+n);
		long long int l1=a[n-1]-a[0];
		long long int l2=b[n-1]-b[0];
		long long int k=max(l1,l2);
		cout<<k*k<<endl;
			
	}
}
